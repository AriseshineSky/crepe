<%- contentFor('pageSection') %>
<style>
  .select,
  #locale {
    width: 100%;
  }
  .like {
    margin-right: 10px;
  }
</style>

<div class="card text-center">
  <div class="card-header">
    Product List
  </div>
  <div class="card-body">
    
    <table
      data-toggle="table"
      data-pagination="true"
    >
      <thead>
        <tr>
          <th>ASIN</th>
          <th>Cycle</th>
          <th>unitsPerBox</th>
          <th>boxD</th>
          <th>maxAvgSales</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <% products.forEach(function(product) { %>
          <tr>
            <td><%= product.asin %></td>
            <td><%= product.cycle %></td>
            <td><%= product.unitsPerBox %></td>
            <td><%= product.box %></td>
            <td><%= product.maxAvgSales %></td>
            <td>
              <a href="/products/<%= product.asin %>/inbounds" target="_blank" type="button" class="btn btn-link">Inbound</a>
              <a href="/products/<%= product.asin %>/plan" target="_blank" type="button" class="btn btn-link">Plan</a>
            </td>
          </tr>
        <% }) %>
      </tbody>
    </table>
      <a id="add" class="btn btn-primary" href="/products/new">
        Add New Product
      </a>
  </div>
</div>




<script>

</script>